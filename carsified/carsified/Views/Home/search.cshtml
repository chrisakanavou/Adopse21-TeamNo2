        $('#cookiewarning').hide();
    } else {
        $('#closecookie').click(function (e) {
            e.preventDefault();
            $('#cookiewarning').fadeOut('fast');
            $.cookie('cookiewarning', '1', { expires: 365, path: '/', domain: 'car.gr' });
            // using document.cookie because $.cookie serializes the array into escaped characters. Max age is one year.
            document.cookie = `policy=["performance","advertising","essential"]; Path=/; Max-Age=31536000; Domain=car.gr`
        });
