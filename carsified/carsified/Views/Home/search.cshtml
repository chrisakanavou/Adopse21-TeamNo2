<script>
    var got_searches = false
    function get_searches() {
        if (got_searches) return
        $.get("/classifieds/searches/", function (data) {
            $("#searches_list").html(data);
        });
    }
    function show_recent(evt, recent_tab) {
        if (recent_tab == "searches_list") get_searches();
        $.cookie("recent_tab", recent_tab, { path: "/", expires: 10 });
        var i, tabcontent, tablinks;
        tabcontent = document.getElementsByClassName("tabcontent");
        for (i = 0; i < tabcontent.length; i++) {
            tabcontent[i].style.display = "none";
        }
        $("#recent_list_button").removeClass("active_recents_tab")
        $("#searches_list_button").removeClass("active_recents_tab")
        $("#" + recent_tab).css("display", "block")
        $("#" + recent_tab + "_button").addClass("active_recents_tab")
    }
    function reload_recents_tab() {
        if ($("#recent_handle").hasClass('showrecent')) {
            $("#searches_list").css("display", "none")
        }
        event = window.event || arguments.callee.caller.arguments[0];
        recent = $.cookie("recent_tab");
        show_recent(event, (recent == undefined) ? "recent_list" : recent);
    }
</script>

