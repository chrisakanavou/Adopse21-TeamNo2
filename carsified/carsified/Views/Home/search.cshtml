            });
        }
        gethits();
        $(".gethitscheck").click(gethits);

        $(".gethits").each(function () {
            var ele = $(this);
            if (ele.val() != "") {
                ele.parent().addClass('selected');
            }
        })
        $(".gethits").change(function () {
            var ele = $(this);
            ele.parent().removeClass('selected');
            if (ele.val() != '') {
                ele.parent().addClass('selected');
            }
            gethits();
        });
        $(".gethitstime").keyup(gethitstime);
        $(".multi-select").on("change", ".gethitsmulticheck", gethitstime);
        $('.deletesearchrow').click(function () {
            var parent = $(this).closest('.searchgroupmultiple');
            parent.find('input').val('').trigger('change');
            parent.find('select').val('').trigger('change');
            parent.hide();
            totalsearchfields--;
            $('.addmoresearches').show();
        });
        $('.addmoresearches').click(function (e) {
            e.preventDefault();
            if (totalsearchfields < 3) {
                $('.searchgroupmultiple:hidden').eq(0).show();
                totalsearchfields++;
                if (totalsearchfields == 3)
                    $(this).hide();
            }
        });

        $("input[name='category']").change(function () {
            categories = []
            $("input[name='category']:checked").each(function () {
                categories.push("category=" + $(this).val())
            })

            $.getJSON("/classifieds/topmakes/cars/?" + categories.join("&"), function (data) {

                makes_select = $(".makes-list")
                if (makes_select === undefined) return
                selected_opts = []

                makes_select.find("input[name='make']:checked").each(function () {
                    selected_opts.push($(this).val())
                })



                function opt(val, is_head, selected) {

                    my_html = '<li class="$class $class2">'.replace("$class", !is_head ? "multi-select-option-searchable" : "").replace("$class2", selected ? "multi-select-option-filled" : "") +
                        '<label for="multi-select-option-make-$value"><input $selected type="checkbox" name="make" value="$value2" title="$name"'.replace("$value", val[1]).replace("$value2", val[1]).replace("$name", val[0]).replace("$selected", selected ? "checked" : "") +
                        'id="multi-select-option-make-$value" class="gethitsmulticheck"><span class="multi-select-option-name">$name</span></label>'.replace("$value", val[1]).replace("$name", val[0]) +
                        '</li>'

                    my_html = '<li class="makeslistli $class $class2">'.replace("$class", !is_head ? "multi-select-option-searchable" : "").replace("$class2", selected ? "multi-select-option-filled" : "") +
                        '<label for="multi-select-option-make-$value"><input $selected type="checkbox" name="make" value="$value2" title="$name"'.replace("$value", val[1]).replace("$value2", val[1]).replace("$name", val[0]).replace("$selected", selected ? "checked" : "") +
                        'id="multi-select-option-make-$value1" class="makeslistinput gethitsmulticheck"><div class="make-logo make-logo-$value2"></div><span class="makeslistname multi-select-option-name">$name</span></label>'.replace("$value1", val[1]).replace("$value2", val[1]).replace("$name", val[0]) +
                        '</li>'

                    return my_html
                }

                var html = ""
                $.each(data["head"], function (key, val) {
                    var s = false
                    for (i = 0; i < selected_opts.length; i++) {
                        if (selected_opts[i] == val[1]) {
                            s = true
                            break
                        }
                    }
                    html += opt(val, true, s)
                });
                html += '<li class="multi-select-option-seperator"></li>'
                $.each(data["tail"], function (key, val) {
                    var s = false
                    for (i = 0; i < selected_opts.length; i++) {
                        if (selected_opts[i] == val[1]) {
                            s = true
                            break
                        }
                    }
                    html += opt(val, false, s)
                });


                makes_select.html(html)
            });

        })

    });

