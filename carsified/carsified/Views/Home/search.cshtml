<script type="text/javascript">

    $("#search").submit(function () {
        $('select').each(function () {
            if ($.trim($(this).val()) == '') {
                $(this).attr("disabled", "disabled")
            }
        })

        $('input').each(function () {
            if ($.trim($(this).val()) == '' || ($(this).hasClass("excludeUrl"))) {
                $(this).attr("disabled", "disabled")
            }
        })
        var formSerial = $('#search').serialize();
        formSerial = formSerial.replace(/[^=&]+=(&|$)/g, '');
        var newurl = window.location.href;
        window.history.pushState({ path: newurl }, '', '?' + formSerial)
    });

    $("#rg").change(function () {
        if ($("#rg").val() == "" || $("#rg").val() == "99999") {
            $("#prefectures_row").hide()
            $("#prefecture").html("")
            $("#prefecture").attr("name", "")
        } else {
            selectedreg = $(this).val();
            $.getJSON("/classifieds/prefectures/99/".replace("99", $(this).val()), function (data) {

                if (selectedreg == 16) {
                    var html = "<option value=''>Επαρχία</option>";
                    $("#label-nomos").hide()
                    $("#label-eparxia").show()
                }
                else {
                    if ($("#label-eparxia:visible").length) {
                        $("#label-eparxia:visible").hide();
                        $("#label-nomos").show()
                    }
                    var html = "<option value=''>Νομός</option>";
                }

                $.each(data, function (key, val) {
                    html += "<option value='$value'>$name</option>".replace("$value", val[0]).replace("$name", val[1])
                });
                $("#prefectures_row").show()
                $("#prefecture").attr("name", "prefecture")
                $("#prefecture").html(html)
            });
        }
    })
    function set_region(i) {
        $('#rg').val(i).change();
        if (i == 16) {
            //if its cyprus change label for nomos to eparxia
            $("#label-nomos").hide()
            $("#label-eparxia").show()
        }
        else {
            if ($("#label-eparxia:visible").length) {
                $("#label-eparxia:visible").hide();
                $("#label-nomos").show()
            }
        }
        gethits();
    }
    function numbersonly(e) {
        var unicode = e.charCode ? e.charCode : e.keyCode
        if (unicode != 8 || unicode != 27) { //backspace and enter
            if (unicode < 48 || unicode > 57) {
                return false
            }
        }
    }
    function validate_number(val, units) {
        var result = '';
        /^[0-9]+$/
    }

    var get_hits_timeout = 0
    function get_hits_time(time) {
        clearTimeout(get_hits_timeout);
        get_hits_timeout = setTimeout(gethits(), time);
    }


    var modelgettimeout
    $(function () {
        $(".models-list").on("click", ".gethits", function () {
            gethits();
        })

        $(".makes-list").on("change", "input[type='checkbox'][name='make']", function () {
            clearTimeout(modelgettimeout)
            modelgettimeout = setTimeout(function () {
                get_models([])
            }, 500)
        })

        if (typeof selected_models !== 'undefined') {
            get_models(selected_models)
        } else {
            get_models([])
        }


        $(".checkbox input:checked").each(function () {
            $(this).parent().addClass('selected');
        })
        $(".radio input:checked").each(function () {
            $(this).parent().addClass('selected');
        })
        $(".checkbox input").change(function () {
            if ($(this)[0].checked) {
                $(this).parent().addClass('selected');
            } else {
                $(this).parent().removeClass('selected');
            }
        })
        $(".radio input").change(function () {
            $(this).parent().parent().parent().find('label').removeClass('selected');
            $(this).parent().parent().parent().find('input:checked').parent().addClass('selected');
        })

        $(".combo2").each(function () {
            var box = $(this);
            var opt = $('<option class="custom"></option>');
            var label = box.find('option[value=""]').html();
            var control = $(''
                + '<div class="input-group">'
                + '<input onkeypress="return numbersonly(event)" placeholder="' + label + '" type="text" class="form-control">'
                + '<div class="input-group-btn">'
                + '<button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown"><span class="caret"></span></button>'
                + '<ul class="dropdown-menu combocontrol pull-right">'
                + '</ul>'
                + '</div><!-- /btn-group -->'
                + '</div><!-- /input-group -->'
            );

            var menu = control.find('.dropdown-menu');
            var input = control.find('input');
            var toggle = control.find('.dropdown-toggle');
            toggle.parent().on('show.bs.dropdown', function () {
                menu.css('width', input.parent().width() + 'px');
                input.focus();
            });

            input.click(function (e) {
                e.stopPropagation();
                if (!menu.parent().hasClass('open')) {
                    toggle.dropdown('toggle');
                    input.focus();
                }
            });
            input.keyup(function (e) {
                if (!menu.parent().hasClass('open')) {
                    toggle.dropdown('toggle');
                    input.focus();
                }
            })
            input.change(function (e) {
                var t = $(this);
                if (t.val() != '') {
                    var val = t.attr('value');
                    var newval = '';
                    for (var i = 0; i < val.length; i++) {
                        if (val[i].match(/[0-9]/)) {
                            newval += val[i];
                        }
                    }
                    t.val(newval);
                    opt.attr('value', newval);
                    box.val(newval);
                }
            })

            var curopt = box.find('option:selected');
            if (curopt.val()) {
                input.val(curopt.html());
            }
            box.find('option').each(function () {
                var t = $(this);
                var li = $('<li></li>');
                var a = $('<a></a>');
                a.click(function () {
                    opt.val(t.val());
                    opt.attr('selected', 'selected');
                    control.parent().addClass('selected');
                    if (t.val() != '') {
                        input.attr('value', t.html());
                    } else {
                        input.attr('value', '');
                    }
                })
                a.html(t.html());
                li.append(a);
                menu.append(li);
            })
            box.prepend(opt);
            box.before(control);
            $(box).hide();
        });



        /*
        $("#postcode").autocomplete("/api/postcodes/", {
            autoFill: true,
            extraParams: {
                //ma: function() { return $("#make_id").val(); }
            }
        });
